pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    // This assumes you have defined 'MAVEN3' as a tool in Jenkins configuration
                    def mvnHome = tool 'MAVEN3' // Replace 'MAVEN3' with the actual name of your Maven tool
                    sh "${mvnHome}/bin/mvn clean package"
                }
            }
        }
    }
}
